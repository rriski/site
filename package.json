{
  "name": "site",
  "license": "MIT",
  "author": "Timo Riski",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "format": "prettier --write src/**/*.js src/**/**/*.js",
    "lint": "eslint --ignore-path .gitignore src/",
    "lint:fix": "eslint --ignore-path .gitignore src/ --fix",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "test": "ava **/*.test.js --verbose"
  },
  "dependencies": {
    "framer-motion": "^10.12.10",
    "gatsby": "5.9.1",
    "gatsby-plugin-image": "^3.9.0",
    "gatsby-plugin-offline": "6.9.0",
    "gatsby-plugin-sharp": "5.9.0",
    "gatsby-plugin-sitemap": "6.9.0",
    "gatsby-plugin-styled-components": "^6.9.0",
    "gatsby-remark-copy-linked-files": "6.9.0",
    "gatsby-remark-images": "7.9.0",
    "gatsby-source-filesystem": "5.9.0",
    "gatsby-transformer-json": "5.9.0",
    "gatsby-transformer-remark": "6.9.0",
    "gatsby-transformer-sharp": "5.9.0",
    "get-scrollbar-width": "1.0.5",
    "intersection-observer": "0.12.2",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.8.0",
    "sharp": "^0.32.1",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "ava": "5.2.0",
    "babel-preset-gatsby": "^3.9.0",
    "chrome-launcher": "0.15.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "favicons": "7.1.2",
    "gatsby-plugin-eslint": "^4.0.4",
    "lighthouse": "10.2.0",
    "mkdirp": "3.0.1",
    "node-gyp": "9.3.1",
    "postcss": "^8.4.23",
    "prettier": "2.8.8"
